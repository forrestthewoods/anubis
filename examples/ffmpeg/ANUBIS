cc_binary(
    name = "ffmpeg",
    lang = "c",
    srcs = RelPaths([
        "FFmpeg/fftools/ffmpeg_dec.c",
        "FFmpeg/fftools/ffmpeg_demux.c",
        "FFmpeg/fftools/ffmpeg_enc.c",
        "FFmpeg/fftools/ffmpeg_filter.c",
        "FFmpeg/fftools/ffmpeg_hw.c",
        "FFmpeg/fftools/ffmpeg_mux.c",
        "FFmpeg/fftools/ffmpeg_mux_init.c",
        "FFmpeg/fftools/ffmpeg_opt.c",
        "FFmpeg/fftools/ffmpeg_sched.c",
        "FFmpeg/fftools/graph/graphprint.c",
        "FFmpeg/fftools/sync_queue.c",
        "FFmpeg/fftools/thread_queue.c",
        "FFmpeg/fftools/textformat/avtextformat.c",
        "FFmpeg/fftools/textformat/tf_compact.c",
        "FFmpeg/fftools/textformat/tf_default.c",
        "FFmpeg/fftools/textformat/tf_flat.c",
        "FFmpeg/fftools/textformat/tf_ini.c",
        "FFmpeg/fftools/textformat/tf_json.c",
        "FFmpeg/fftools/textformat/tf_mermaid.c",
        "FFmpeg/fftools/textformat/tf_xml.c",
        "FFmpeg/fftools/textformat/tw_avio.c",
        "FFmpeg/fftools/textformat/tw_buffer.c",
        "FFmpeg/fftools/textformat/tw_stdout.c",
        "FFmpeg/fftools/resources/resman.c",
        "FFmpeg/fftools/cmdutils.c",
        "FFmpeg/fftools/opt_common.c",
        "FFmpeg/fftools/ffmpeg.c",
    ]) + select(
        (target_platform,) => {
            # ffplay requires SDL2
            (linux) = RelPaths([
                #"FFmpeg/fftools/ffplay.c",
                #"FFmpeg/fftools/ffplay_renderer.c",
            ]),
            (windows) = RelPaths([]),
        }
    ),
    include_dirs = [ 
        RelPath("FFmpeg"),
        RelPath("FFmpeg/compat/stdbit"),
    ],
    deps = Targets([
        "//examples/ffmpeg:avdevice",
        "//examples/ffmpeg:avfilter",
        "//examples/ffmpeg:avformat",
        "//examples/ffmpeg:ffmpeg_resources",
    ]),
    libraries = select(
        (target_platform) => {
            (windows) = [
                "Shell32.lib",
                "Ws2_32.lib",
                "mfuuid.lib",
                "oldnames.lib",
                "user32.lib",
                "ole32.lib",
                "oleaut32.lib",
                "strmiids.lib",
                "shlwapi.lib",
                "ncrypt.lib",
                "crypt32.lib",
                "secur32.lib",
                "Gdi32.lib",
                "Vfw32.lib",
            ],
            default = [],
        }
    ),
)

cc_binary(
    name = "bin2c",
    lang = "c",
    srcs = RelPaths([
        "FFmpeg/ffbuild/bin2c.c",
    ]),
)

# Code generation using bin2c
# bin2c converts binary files (HTML, CSS) into C source arrays
# Usage: bin2c <variable_name> <input_file> <output_file>
# Each inner array is a separate command invocation that runs in parallel.
anubis_cmd(
    name = "generate_resources",
    tool = Target("//examples/ffmpeg:bin2c"),
    args = [
        [RelPath("FFmpeg/fftools/resources/graph.html"), RelPath("FFmpeg/fftools/resources/graph.html.c"), "graph_html"],
        [RelPath("FFmpeg/fftools/resources/graph.css"), RelPath("FFmpeg/fftools/resources/graph.css.c"), "graph_css"],
    ],
)

# Static library containing generated resources
# This library depends on the anubis_cmd rule to ensure code generation completes
# before compilation starts
cc_static_library(
    name = "ffmpeg_resources",
    lang = "c",
    srcs = RelPaths([
        "FFmpeg/fftools/resources/graph.html.c",
        "FFmpeg/fftools/resources/graph.css.c",
    ]),
    deps = Targets([
        "//examples/ffmpeg:generate_resources",
    ]),
    public_include_dirs = [
        RelPath("generated"),
    ],
)


cc_static_library(
    name = "avdevice",
    lang = "c",
    srcs = RelPaths([ 
        "FFmpeg/libavdevice/alldevices.c",
        "FFmpeg/libavdevice/avdevice.c",
        "FFmpeg/libavdevice/lavfi.c",
        "FFmpeg/libavdevice/utils.c",
        "FFmpeg/libavdevice/version.c",
    ]) + select(
        (target_platform,) => {
            (linux) = RelPaths([
            ]),
            (windows) = RelPaths([
                "FFmpeg/libavdevice/dshow.c",
                "FFmpeg/libavdevice/dshow_common.c",
                "FFmpeg/libavdevice/dshow_crossbar.c",
                "FFmpeg/libavdevice/dshow_enummediatypes.c",
                "FFmpeg/libavdevice/dshow_enumpins.c",
                "FFmpeg/libavdevice/dshow_filter.c",
                "FFmpeg/libavdevice/dshow_pin.c",
                "FFmpeg/libavdevice/gdigrab.c",
                "FFmpeg/libavdevice/lavfi.c",
                "FFmpeg/libavdevice/vfwcap.c",
            ]),
        }
    ),,
    deps = Targets([
        "//examples/ffmpeg:avutil",
    ]),
    public_include_dirs = [
        RelPath("FFmpeg"),
        RelPath("FFmpeg/libavdevice"),
    ],
    private_compiler_flags = [
        "-Wno-deprecated-declarations",
        "-Wno-implicit-function-declaration",
    ],
    private_defines = [
        "HAVE_AV_CONFIG_H=1",
    ],
)

