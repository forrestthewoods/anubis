cc_binary(
    name = "main",
    lang = "cpp",
    srcs = [ RelPath("src/main.cpp") ]
    deps = Targets(["//examples/staticlib_cpp:foo"]),
)

cc_static_library(
    name = "foo",
    lang = "cpp",
    srcs = [ RelPath("src/foo.cpp") ],
    public_include_dirs = [ RelPath("include") ],
    deps = Targets([]),
)
